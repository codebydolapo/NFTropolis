{
  "name": "enefti",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "hardhat compile",
    "test": "hardhat test test/unit/*.spec.js",
    "test-staging": "hardhat test test/staging/*.spec.js --network goerli",
    "deploy": "hardhat run scripts/deployment/main.js",
    "hardhatlint": "solhint 'contracts/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "fuzzing": "docker run -it --rm -v $PWD:/src trailofbits/eth-security-toolbox"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "dotenv": "^16.0.3",
    "hardhat-gas-reporter": "^1.0.8",
    "next": "12.2.4",
    "os-browserify": "^0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@chainlink/contracts": "0.5.1",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/node": "^18.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "chai": "^4.3.6",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "ethers": "^5.7.1",
    "hardhat": "^2.11.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.sol": [
      "solhint"
    ]
  }
